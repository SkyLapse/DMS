<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:clr="clr-namespace:System;assembly=mscorlib">
    <clr:Boolean x:Key="IsSkinDictionary">true</clr:Boolean>
    
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF007ACC"/>
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="CaptionTextBrush" Color="Blue"/>
    <SolidColorBrush x:Key="LinkColorBrush" Color="#FF0645AD"/>
    <SolidColorBrush x:Key="EyeLogoBrush" Color="#FF333333"/>
</ResourceDictionary>